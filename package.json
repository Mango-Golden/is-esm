{
  "name": "@busymango/is-esm",
  "version": "0.1.3",
  "description": "type narrowing for typescript",
  "type": "module",
  "files": [
    "dist",
    "index.d.ts",
    "src/*.d.ts"
  ],
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "tsc && vite build",
    "preversion": "pnpm run build",
    "coverage": "vitest run --coverage",
    "update:ts": "pnpm add tslib@latest typescript@latest",
    "update:vite": "pnpm add vite@latest vitest@latest @vitest/coverage-v8@latest"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.3",
    "jsdom": "^22.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.0.4",
    "vitest": "^0.33.0"
  }
}
